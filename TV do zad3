package com.company;

public class TV {
    boolean on;
    int volume;
    int channel;

    public TV() {
        this.channel = 12;
        this.volume = 11;
        this.on = false;
    }

    public boolean check_tv_on(){
        if (this.on == false) {
            System.out.println("Telewizor jest wyłączony");
        }
        return this.on;
    }
//Norbert Kulik
}
